package com.internousdev.webproj4.action;

import java.util.ArrayList;
import java.util.List;

import com.internousdev.webproj4.dao.HelloStrutsDAO;
import com.internousdev.webproj4.dto.HelloStrutsDTO;
import com.opensymphony.xwork2.ActionSupport;

public class HelloStrutsAction extends ActionSupport{

	private List<HelloStrutsDTO> DTOList = new ArrayList<HelloStrtsDTO>();

	public String execute(){
		String ret=ERROR;
		HelloStrutsDAO dao=new HelloStrutsDAO();

		DTOList=dao.select();

		if(DTOList.size() > 0) {
			ret=SUCCESS;
		}else{
			ret=ERROR;
		}return ret;
	}public List<HelloStrutsDTO>getDTOList(){
		return DTOList;
	}public void setDTOList(List<HelloStrutsDTO>helloStrutsDTOList){
		DTOList=DTOList;
	}
}
