package abc;

public class Main1020 {
	public static void main(String[] args) {
		String[][] array = new String[2][];
		array[0] = new String[2];
		array[1] = new String[5];
		int i = 97;
		for (int a = 0; a < array.length; a++) {
			System.out.println("☆");
			for (int b = 0; b < array.length; b++) {
				System.out.println("★");

				array[a][b] = "" + i;
				i++;
			}
		}
		for (String[] tmp : array) {
			for (String s : tmp) {
				System.out.print(s + " ");
			}
			System.out.println();
		}
	}

}
