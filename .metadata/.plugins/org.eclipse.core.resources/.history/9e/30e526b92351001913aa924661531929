package abc;

public class Practice26_Q3 {
    public static void main(String[] args) {
        System.out.println(getSum(3, 1));
    }

    static int getSum(int start, int end) {
        int sum = 0;
        for(int i = start; i <= end; i++) {
            sum += i;
        }
        return sum;
    }
}